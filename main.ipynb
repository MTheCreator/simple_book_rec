    "    tokenized_desc = lambda book: {word.lower() for word in book.desc.split(' ') if verify_word(word)}\n",
    "\n",
    "    return { book.isbn : tokenized_title(book) | tokenized_desc(book) | set(book.genre) for book in books_list }\n",
    "    "
